{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/warnings",
    "../eslint-config.json"
  ],
  "plugins": ["only-warn"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off"
  },
  "settings": {
    "import/resolver": {
      "typescript": {
        "project": "server/tsconfig.json"
      }
    }
  }
}
